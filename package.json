{
  "name": "spearmint",
  "version": "0.11.0",
  "description": "An open-source developer tool that simplifies testing and hopes to help increase awareness about web accessibility.",
  "main": "public/electron.jsx",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "start": "electron-forge start",
    "start-dev": "concurrently \"electron .\" \"nodemon server/server.js\"",
    "watch": "webpack --config webpack.config.ts --watch",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dev": "npm run watch & npm run start-dev",
    "rebuild": "electron-rebuild -f -w node-pty",
    "docker": "electron . --no-sandbox"
  },
  "build": {
    "appId": "spearmint",
    "productName": "spearmint",
    "mac": {
      "icon": "./public/icon.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./public/icon.png"
    },
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",
      "*.js"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "keywords": [],
  "author": "team spearmint",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@capacitor/screen-reader": "^1.0.6",
    "@codemirror/lang-javascript": "^6.0.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@uiw/react-codemirror": "^4.8.1",
    "babel-loader": "^8.2.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "codemirror": "^6.0.0",
    "concurrently": "^6.4.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.2.0",
    "depcheck": "^1.4.3",
    "dotenv": "^10.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "js-beautify": "^1.14.0",
    "mongoose": "^6.0.14",
    "node-fetch": "^3.2.0",
    "node-pty": "^0.10.1",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-icons": "^4.3.1",
    "react-modal": "^3.14.4",
    "react-transition-group": "^4.4.2",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "save-dev": "0.0.1-security",
    "snyk": "^1.790.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "xterm": "^4.15.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-for-react": "^1.0.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "axe-core": "^4.3.5",
    "electron": "^15.5.5",
    "electron-rebuild": "^3.2.7",
    "electron-reloader": "^1.2.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack-dev-server": "^4.9.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "spearmint"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin", "linux", "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "spearmint"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
